//C++ password program

#include<iostream>
#include<string.h>
using namespace std;
int main()
{
    char pass[20], ePass[20];
    int numOne, numTwo, multiply;
    cout<<"Create a Password: ";
    cin>>pass;
    cout<<"\nEnter Two Numbers to multiply: ";
    cin>>numOne>>numTwo;
    cout<<"\nEnter the Password to See the Answer: ";
    cin>>ePass;
    if(!strcmp(pass, ePass))
    {
        multiply = numOne * numTwo;
        cout<<endl<<numOne<<" * "<<numTwo<<" = "<<multiply;
    }
    else
        cout<<"\nSorry! The password you enter is incorrect!";
    cout<<endl;
    return 0;
}
